<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/10/nuspec.xsd">
  <metadata>
    <id>XLabs.Core</id>
    <version>2.3.0-pre04</version>
    <title>XLabs - Core</title>
    <authors>XLabs Team</authors>
    <owners>XLabs Team</owners>
    <licenseUrl>https://github.com/XLabs/Xamarin-Forms-Labs/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/XLabs/Xamarin-Forms-Labs</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/XLabs/Xamarin-Forms-Labs/master/Design/Icons/nuget/icon_nuget.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>This packages implements core base functionality that all of XLabs uses.  DO NOT INCLUDED THIS DIRECTLY.</description>
    <summary>XLabs is a open source project that aims to provide a powerfull and cross platform set of controls tailored to work with Xamarin Forms.</summary>
    <releaseNotes>Updated for Xamarin Forms 2.3</releaseNotes>
    <copyright>Copyright © 2016 XLabs Team</copyright>
    <tags>Xamarin XLabs</tags>
    <dependencies>
      <group targetFramework="NETStandard1.0"/>
      <group targetFramework=".NETFramework4.5"/>
      <group targetFramework=".NETPortable,Version=v4.5,Profile=Profile78"/>
      <group targetFramework=".NETPortable,Version=v4.5,Profile=Profile111"/>
      <group targetFramework=".NETPortable,Version=v4.5,Profile=Profile259"/>
	  <group targetFramework="MonoAndroid"/>
	  <group targetFramework="MonoTouch"/>
	  <group targetFramework="Xamarin.iOS10"/>
	  <group targetFramework="Xamarin.Mac"/>
    </dependencies>
  </metadata>
  <files>
    <!-- Content Files -->
    <!-- file src="..\..\docs\XLabs.Core.Readme.md" target="content" -->
    <!-- Assemblies: PCL Profile 259 + Xamarin -->
    <file src="..\src\Core\bin\$configuration$\XLabs.Core.dll" target="lib\portable-net45+win8+wpa81+wp8+monoandroid+monotouch+xamarinios10+xamarinmac" />
    <file src="..\src\Core\bin\$configuration$\XLabs.Core.pdb" target="lib\portable-net45+win8+wpa81+wp8+monoandroid+monotouch+xamarinios10+xamarinmac" />

    <!-- Assemblies: .NET Standard 1.0 -->
    <file src="..\src\Core\bin\$configuration$\XLabs.Core.dll" target="lib\netstandard1.0" />
    <file src="..\src\Core\bin\$configuration$\XLabs.Core.pdb" target="lib\netstandard1.0" />
  </files>
</package>